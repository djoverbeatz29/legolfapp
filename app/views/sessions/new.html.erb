<h1>Log In</h1>

<%= flash[:error] %>

<%= form_with url: "/sessions" do %>
    <p>Email: <%= text_field_tag :email %></p>
    <p>Password: <%= password_field_tag :password %></p>
    <%= submit_tag "Submit" %>
    <button class="ui button"><%= link_to "Back", new_golfer_path %></button>
<% end %>

